  }
  
  /**
  *
  * Returns the fee taken out of every transaction to be kept in the Contract's Address
  *
  */
  function _calculateSafemoonFee(uint256 amount) private view returns(uint256) {
     return amount.mul(_sfmFee).div(10**2);
  }
